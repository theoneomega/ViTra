<div class="page-header">
  <%= link_to sub_brands_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_sub_brand_path(@sub_brand), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show sub_brand</h1>
</div>

<dl class="dl-horizontal">
  <dt>Id:</dt>
  <dd><%= @sub_brand.id %></dd>

  <dt>Description:</dt>
  <dd><%= @sub_brand.description %></dd>

  <dt>Brand:</dt>
  <dd><%= @sub_brand.brand_id %></dd>

</dl>
