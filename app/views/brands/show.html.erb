<div class="page-header">
  <%= link_to brands_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_brand_path(@brand), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show brand</h1>
</div>

<dl class="dl-horizontal">
  <dt>Id:</dt>
  <dd><%= @brand.id %></dd>

  <dt>Description:</dt>
  <dd><%= @brand.description %></dd>

</dl>
