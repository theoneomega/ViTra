<div class="page-header">
  <%= link_to drugs_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_drug_path(@drug), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show drug</h1>
</div>

<dl class="dl-horizontal">
  <dt>Id:</dt>
  <dd><%= @drug.id %></dd>

  <dt>Kind:</dt>
  <dd><%= @drug.kind %></dd>

  <dt>Quantity:</dt>
  <dd><%= @drug.quantity %></dd>

  <dt>Observations:</dt>
  <dd><%= @drug.observations %></dd>

  <dt>Iph:</dt>
  <dd><%= @drug.iph_id %></dd>

</dl>
