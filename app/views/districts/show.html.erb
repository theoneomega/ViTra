<div class="page-header">
  <%= link_to districts_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_district_path(@district), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show district</h1>
</div>

<dl class="dl-horizontal">
  <dt>Id:</dt>
  <dd><%= @district.id %></dd>

  <dt>Description:</dt>
  <dd><%= @district.description %></dd>

</dl>
