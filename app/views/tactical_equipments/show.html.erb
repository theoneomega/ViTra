<div class="page-header">
  <%= link_to tactical_equipments_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_tactical_equipment_path(@tactical_equipment), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show tactical_equipment</h1>
</div>

<dl class="dl-horizontal">
  <dt>Id:</dt>
