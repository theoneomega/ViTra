<div class="page-header">
  <%= link_to patrols_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_patrol_path(@patrol), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show patrol</h1>
</div>

<dl class="dl-horizontal">
  <dt>Id:</dt>
  <dd><%= @patrol.id %></dd>

  <dt>Description:</dt>
  <dd><%= @patrol.description %></dd>

</dl>
