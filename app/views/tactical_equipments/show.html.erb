  <dt>Kind:</dt>
  <dd><%= @tactical_equipment.kind %></dd>

  <dt>Brand:</dt>
  <dd><%= @tactical_equipment.brand %></dd>

  <dt>Model:</dt>
  <dd><%= @tactical_equipment.model %></dd>

  <dt>Inventary:</dt>
  <dd><%= @tactical_equipment.inventary %></dd>

  <dt>Serial:</dt>
  <dd><%= @tactical_equipment.serial %></dd>

  <dt>Quantity:</dt>
  <dd><%= @tactical_equipment.quantity %></dd>

  <dt>Observations:</dt>
  <dd><%= @tactical_equipment.observations %></dd>

  <dt>Iph:</dt>
  <dd><%= @tactical_equipment.iph_id %></dd>

</dl>
