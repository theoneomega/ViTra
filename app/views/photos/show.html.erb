<div class="page-header">
  <%= link_to photos_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_photo_path(@photo), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show photo</h1>
</div>

<dl class="dl-horizontal">
  <dt>Id:</dt>
  <dd><%= @photo.id %></dd>

  <dt>Description:</dt>
  <dd><%= @photo.description %></dd>

  <dt>Image file name:</dt>
  <dd><%= @photo.image_file_name %></dd>

  <dt>Image content type:</dt>
  <dd><%= @photo.image_content_type %></dd>

  <dt>Image file size:</dt>
  <dd><%= @photo.image_file_size %></dd>

  <dt>Image updated at:</dt>
  <dd><%= @photo.image_updated_at %></dd>

  <dt>Person:</dt>
  <dd><%= @photo.person_id %></dd>

</dl>
