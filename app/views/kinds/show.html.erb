<div class="page-header">
  <%= link_to kinds_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_kind_path(@kind), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show kind</h1>
</div>

<dl class="dl-horizontal">
  <dt>Id:</dt>
  <dd><%= @kind.id %></dd>

  <dt>Description:</dt>
  <dd><%= @kind.description %></dd>

</dl>
