<div class="page-header">
  <%= link_to officers_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_officer_path(@officer), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show officer</h1>
</div>

<dl class="dl-horizontal">
  <dt>Id:</dt>
  <dd><%= @officer.id %></dd>

  <dt>First name:</dt>
  <dd><%= @officer.first_name %></dd>

  <dt>Last name1:</dt>
  <dd><%= @officer.last_name1 %></dd>

  <dt>Last name2:</dt>
  <dd><%= @officer.last_name2 %></dd>

  <dt>Patrol:</dt>
  <dd><%= @officer.patrol.description %></dd>

</dl>
