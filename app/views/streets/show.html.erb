<div class="page-header">
  <%= link_to streets_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_street_path(@street), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show street</h1>
</div>

<dl class="dl-horizontal">
  <dt>Id:</dt>
  <dd><%= @street.id %></dd>

  <dt>Description:</dt>
  <dd><%= @street.description %></dd>

</dl>
