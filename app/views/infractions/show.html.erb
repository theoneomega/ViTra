<div class="page-header">
  <%= link_to infractions_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_infraction_path(@infraction), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
