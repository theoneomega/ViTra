<div class="page-header">
  <%= link_to official_qualifiers_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_official_qualifier_path(@official_qualifier), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show official_qualifier</h1>
</div>

<dl class="dl-horizontal">
  <dt>Id:</dt>
  <dd><%= @official_qualifier.id %></dd>

  <dt>Name:</dt>
  <dd><%= @official_qualifier.name %></dd>

</dl>
