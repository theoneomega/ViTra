<div class="page-header">
  <%= link_to new_iph_path, :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-plus"></span>
    New Iph
  <% end %>
  <h1>Listing iphs</h1>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
            <th>Id</th>
            <th>Office number</th>
            <th>Event date</th>
            <th>Patrol</th>
            <th>Officer</th>
            <th>Infraction</th>
            <th>Addressed to</th>
            <th>Operative</th>
            <th>Operative name</th>
            <th>Coordinator</th>
            <th>Commander</th>
            <th>State</th>
            <th>Twonship</th>
            <th>District</th>
            <th>Sector</th>
            <th>Shift</th>
            <th>Suburb</th>
            <th>Street</th>
            <th>Exterior number</th>
            <th>Interior number</th>
            <th>Zip</th>
            <th>Between streets</th>
            <th>References</th>
            <th>Road</th>
            <th>Road name</th>
            <th>Kilometer</th>
            <th>Facts description</th>
            <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @iphs) do |iph| %>
            <td><%= iph.id %></td>
            <td><%= iph.office_number %></td>
            <td><%= iph.event_date %></td>
            <td><%= iph.patrol_id %></td>
            <td><%= iph.officer_id %></td>
            <td><%= iph.infraction_id %></td>
            <td><%= iph.addressed_to %></td>
            <td><%= iph.operative %></td>
            <td><%= iph.operative_name %></td>
            <td><%= iph.coordinator_id %></td>
            <td><%= iph.commander_id %></td>
            <td><%= iph.state %></td>
            <td><%= iph.township_id %></td>
            <td><%= iph.district_id %></td>
            <td><%= iph.sector_id %></td>
            <td><%= iph.shift %></td>
            <td><%= iph.suburb_id %></td>
            <td><%= iph.street_id %></td>
            <td><%= iph.exterior_number %></td>
            <td><%= iph.interior_number %></td>
            <td><%= iph.zip %></td>
            <td><%= iph.between_streets %></td>
            <td><%= iph.references %></td>
            <td><%= iph.road %></td>
            <td><%= iph.road_name %></td>
            <td><%= iph.kilometer %></td>
            <td><%= iph.facts_description %></td>
            <td><%= link_to 'Show', iph %></td>
        <td><%= link_to 'Edit', edit_iph_path(iph) %></td>
        <td><%= link_to 'Destroy', iph, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      <% end %>
    </tbody>
  </table>
</div>
