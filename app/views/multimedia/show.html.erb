<div class="page-header">
  <%= link_to multimedia_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_multimedium_path(@multimedium), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show multimedium</h1>
</div>

<dl class="dl-horizontal">
  <dt>Id:</dt>
  <dd><%= @multimedium.id %></dd>

  <dt>Kind:</dt>
  <dd><%= @multimedium.kind %></dd>

  <dt>Quantity:</dt>
  <dd><%= @multimedium.quantity %></dd>

  <dt>Description:</dt>
  <dd><%= @multimedium.description %></dd>

  <dt>Iph:</dt>
  <dd><%= @multimedium.iph_id %></dd>

</dl>
