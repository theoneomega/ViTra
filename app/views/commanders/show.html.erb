<div class="page-header">
  <%= link_to commanders_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_commander_path(@commander), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show commander</h1>
</div>

<dl class="dl-horizontal">
  <dt>Id:</dt>
  <dd><%= @commander.id %></dd>

  <dt>First name:</dt>
  <dd><%= @commander.first_name %></dd>

  <dt>Last name1:</dt>
  <dd><%= @commander.last_name1 %></dd>

  <dt>Last name2:</dt>
  <dd><%= @commander.last_name2 %></dd>

</dl>
